{
    "name": "usamamuneerchaudhary/country-city-state",
    "description": "Country City State Data Provider for Laravel 12+",
    "keywords": [
        "usamamuneerchaudhary",
        "country-state-city",
        "country-city-state",
        "countries",
        "country",
        "laravel",
        "state",
        "states",
        "city",
        "cities"
    ],
    "homepage": "https://github.com/usamamuneerchaudhary/country-city-state.git",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Usama Muneer",
        "email": "hello@usamamuneer.me",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.3|^8.4",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "UsamaMuneerChaudhary\\CountryCityState\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UsamaMuneerChaudhary\\CountryCityState\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "UsamaMuneerChaudhary\\CountryCityState\\CountryCityStateServiceProvider"
            ],
            "aliases": {
                "CountryCityState": "UsamaMuneerChaudhary\\CountryCityState\\CountryCityStateFacade"
            }
        }
    }
}
